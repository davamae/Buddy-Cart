<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".shoppingPage">

    <ImageButton
        android:id="@+id/watBtn"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:scaleType="centerCrop"
        android:src="@drawable/watermellon"
        app:layout_constraintStart_toEndOf="@+id/shiBtn"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <ImageButton
        android:id="@+id/shiBtn"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:scaleType="centerCrop"
        android:src="@drawable/shirt"
        app:layout_constraintStart_toEndOf="@+id/soapBtn"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <ImageButton
        android:id="@+id/soapBtn"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:scaleType="centerCrop"
        android:src="@drawable/soap"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <ImageButton
        android:id="@+id/honBtn"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:scaleType="centerCrop"
        android:src="@drawable/honey"
        app:layout_constraintStart_toEndOf="@+id/eggBtn"
        app:layout_constraintTop_toBottomOf="@+id/moreItemsPageBtn" />

    <ImageButton
        android:id="@+id/eggBtn"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:scaleType="centerCrop"
        android:src="@drawable/egg"
        app:layout_constraintStart_toEndOf="@+id/banBtn"
        app:layout_constraintTop_toBottomOf="@+id/moreItemsPageBtn" />

    <ImageButton
        android:id="@+id/banBtn"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:scaleType="centerCrop"
        android:src="@drawable/banana"
        app:layout_constraintStart_toEndOf="@+id/appleBtn"
        app:layout_constraintTop_toBottomOf="@+id/moreItemsPageBtn" />

    <View
        android:id="@+id/bottomWhiteSheet2"
        android:layout_width="451dp"
        android:layout_height="78dp"
        android:background="@android:color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/cartBtn"
        android:layout_width="57dp"
        android:layout_height="57dp"
        android:layout_marginEnd="168dp"
        android:layout_marginBottom="16dp"
        android:backgroundTint="#00FFFFFF"
        android:scaleType="centerInside"
        android:src="@drawable/cart"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageButton
        android:id="@+id/houseImageButton3"
        android:layout_width="57dp"
        android:layout_height="57dp"
        android:layout_marginEnd="324dp"
        android:layout_marginBottom="16dp"
        android:backgroundTint="#00FFFFFF"
        android:scaleType="centerInside"
        android:src="@drawable/house"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageButton
        android:id="@+id/mapBtn1"
        android:layout_width="57dp"
        android:layout_height="57dp"
        android:layout_marginEnd="248dp"
        android:layout_marginBottom="16dp"
        android:backgroundTint="#00FFFFFF"
        android:scaleType="centerInside"
        android:src="@drawable/maps"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageButton
        android:id="@+id/bellimageButton3"
        android:layout_width="57dp"
        android:layout_height="57dp"
        android:layout_marginEnd="92dp"
        android:layout_marginBottom="16dp"
        android:backgroundTint="#00FFFFFF"
        android:scaleType="centerInside"
        android:src="@drawable/chat_icon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageButton
        android:id="@+id/personImageButton3"
        android:layout_width="57dp"
        android:layout_height="57dp"
        android:layout_margin="16dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="16dp"
        android:backgroundTint="#00FFFFFF"
        android:scaleType="centerInside"
        android:src="@drawable/person"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <EditText
        android:id="@+id/seachbarMain"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="16dp"
        android:drawableLeft="?android:attr/actionModeWebSearchDrawable"
        android:ems="10"
        android:hint="Search"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/favBtn"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:backgroundTint="@color/off_white"
        android:drawableLeft="@drawable/baseline_favorite_border_24"
        android:paddingLeft="4dp"
        android:text="Favourites"
        android:textColor="@color/black"
        android:textSize="12sp"
        app:iconSize="18dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/seachbarMain"
        />

    <Button
        android:id="@+id/historyBtn"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:backgroundTint="@color/off_white"
        android:drawableLeft="@drawable/baseline_history_24"
        android:paddingLeft="4dp"
        android:text="History"
        android:textColor="@color/black"
        android:textSize="12sp"
        app:layout_constraintStart_toEndOf="@+id/favBtn"
        app:layout_constraintTop_toBottomOf="@+id/seachbarMain"
        />

    <Button
        android:id="@+id/ordersBtn"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:backgroundTint="@color/off_white"
        android:drawableLeft="@drawable/baseline_attach_money_24"
        android:paddingLeft="4dp"
        android:text="Sales"
        android:textColor="@color/black"
        android:textSize="12sp"
        app:layout_constraintStart_toEndOf="@+id/historyBtn"
        app:layout_constraintTop_toBottomOf="@+id/seachbarMain"
        />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:adjustViewBounds="true"
        android:scaleX=".9"
        android:scaleY=".9"
        android:src="@drawable/banner"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/historyBtn" />

    <Button
        android:id="@+id/moreItemsPageBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:backgroundTint="@color/off_white"
        android:drawableRight="@drawable/baseline_keyboard_arrow_right_24"
        android:text="Items"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView5" />

    <ImageButton
        android:id="@+id/appleBtn"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:scaleType="centerCrop"
        android:src="@drawable/apple"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/moreItemsPageBtn" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Apple"
        app:layout_constraintEnd_toEndOf="@+id/appleBtn"
        app:layout_constraintStart_toStartOf="@+id/appleBtn"
        app:layout_constraintTop_toBottomOf="@+id/appleBtn" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Banana"
        app:layout_constraintEnd_toEndOf="@+id/banBtn"
        app:layout_constraintStart_toStartOf="@+id/banBtn"
        app:layout_constraintTop_toBottomOf="@+id/banBtn" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Eggs"
        app:layout_constraintEnd_toEndOf="@+id/eggBtn"
        app:layout_constraintStart_toStartOf="@+id/eggBtn"
        app:layout_constraintTop_toBottomOf="@+id/eggBtn" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Honey"
        app:layout_constraintEnd_toEndOf="@+id/honBtn"
        app:layout_constraintStart_toStartOf="@+id/honBtn"
        app:layout_constraintTop_toBottomOf="@+id/honBtn" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Soap"
        app:layout_constraintEnd_toEndOf="@+id/soapBtn"
        app:layout_constraintStart_toStartOf="@+id/soapBtn"
        app:layout_constraintTop_toBottomOf="@+id/soapBtn" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Shirt"
        app:layout_constraintEnd_toEndOf="@+id/shiBtn"
        app:layout_constraintStart_toStartOf="@+id/shiBtn"
        app:layout_constraintTop_toBottomOf="@+id/shiBtn" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Watermellon"
        app:layout_constraintEnd_toEndOf="@+id/watBtn"
        app:layout_constraintStart_toStartOf="@+id/watBtn"
        app:layout_constraintTop_toBottomOf="@+id/watBtn" />

</androidx.constraintlayout.widget.ConstraintLayout>